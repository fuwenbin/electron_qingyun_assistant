# 环境变量配置示例
# 复制此文件为 .env.local 并填入真实值

# 应用基础信息
VITE_APP_NAME=剪辑助手
VITE_APP_VERSION=1.0.0

# Django API 配置（主要登录方式）
VITE_DJANGO_API_URL=http://localhost:8000

# Stack Auth Configuration（已保留但不使用）
# 请在 https://stack-auth.com 注册并获取你的项目配置
# VITE_STACK_PROJECT_ID=your-project-id
# VITE_STACK_PUBLISHABLE_KEY=your-publishable-key

# 环境配置
NODE_ENV=development

# 开发服务器配置 (仅开发环境需要)
# VITE_DEV_SERVER_URL=http://localhost:3000

# 功能开关
# VITE_DEBUG_MODE=true
# VITE_ENABLE_DEV_TOOLS=true
