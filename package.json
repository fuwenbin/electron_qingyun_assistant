{
  "name": "video-editor-helper",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "electron:dev_venv": "node scripts/dev-with-venv.js",
    "electron:dev": "npm run setup:ffmpeg && concurrently -k \"vite\" \"wait-on http://localhost:3000 && tsc -p tsconfig.node.json && cross-env VITE_DEV_SERVER_URL=http://localhost:3000 electron .\"",
    "electron:build": "npm run setup:ffmpeg && npm run setup:python:portable && npm run build:renderer && npm run build:main && electron-builder",
    "electron:build:mac": "npm run setup:ffmpeg && npm run setup:python:portable && npm run build:renderer && npm run build:main && electron-builder --mac",
    "electron:build:mac:optimized": "node scripts/build-mac-optimized.js",
    "electron:build:win": "npm run setup:ffmpeg && npm run setup:python:portable && npm run build:renderer && npm run build:main && electron-builder --win",
    "electron:build:win:optimized": "node scripts/build-win-optimized.js",
    "electron:build:win:portable": "node scripts/build-win-portable.js",
    "electron:preview": "npm run build:renderer && npm run build:main && electron .",
    "build:renderer": "vue-tsc --noEmit && vite build",
    "build:main": "tsc -p tsconfig.node.json",
    "setup:ffmpeg": "node scripts/setup-ffmpeg.js",
    "setup:python": "node scripts/setup-python.js setup",
    "setup:python:portable": "node scripts/setup-python.js portable",
    "test:edge-tts": "node scripts/test-edge-tts.js",
    "verify:edge-only": "node scripts/verify-edge-tts-only.js",
    "type-check": "vue-tsc --noEmit",
    "type-check:main": "tsc -p tsconfig.node.json --noEmit",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "postinstall": "electron-builder install-app-deps && npm run setup:ffmpeg && npm run setup:python"
  },
  "dependencies": {
    "@alicloud/pop-core": "^1.8.0",
    "@ant-design/icons-vue": "^7.0.1",
    "@electron/remote": "^2.1.2",
    "@ffmpeg/core": "^0.12.10",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@quasar/extras": "^1.16.17",
    "@stackframe/stack": "^2.8.41",
    "alibabacloud-nls": "^1.0.4",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.8.4",
    "buffer": "^6.0.3",
    "chromium-bidi": "^5.2.0",
    "crypto-browserify": "^3.12.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "edge-tts": "^1.0.1",
    "electron-log": "^5.4.0",
    "file-saver": "^2.0.5",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.3.0",
    "iconv-lite": "^0.6.3",
    "js-base64": "^3.7.7",
    "p-limit": "^6.2.0",
    "path": "^0.12.7",
    "pinia": "^2.1.6",
    "playwright": "^1.52.0",
    "playwright-extra": "^4.3.6",
    "playwright-stealth": "^0.0.1",
    "process": "^0.11.10",
    "qs": "^6.14.0",
    "quasar": "^2.18.1",
    "safer-buffer": "^2.1.2",
    "sql.js": "^1.13.0",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "uuid": "^11.1.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.9.0",
    "@types/crypto-js": "^4.2.2",
    "@types/electron": "^1.4.38",
    "@types/node": "^22.15.3",
    "@types/qs": "^6.9.18",
    "@types/sql.js": "^1.4.9",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-vue": "^4.6.2",
    "concurrently": "^9.1.2",
    "cross-env": "^10.0.0",
    "electron": "^36.2.1",
    "electron-builder": "^24.6.3",
    "electron-rebuild": "^3.2.9",
    "sass": "^1.69.5",
    "typescript": "^5.8.3",
    "vite": "^4.5.13",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^2.2.10",
    "wait-on": "^8.0.3"
  },
  "main": "dist-electron/main.js"
}
